{
  "name": "vscode-abap-remote-fs-server",
  "version": "1.0.0",
  "description": "Language server for ABAP remote filesystem",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcellourbani/vscode_abap_remote_fs.git"
  },
  "keywords": [
    "ABAP"
  ],
  "author": "Marcello Urbani",
  "license": "MIT",
  "dependencies": {
    "abap-adt-api": "^0.5.20",
    "html-entities": "^1.2.1",
    "lodash": "^4.17.11",
    "vscode-languageserver": "^4.4.2"
  },
  "devDependencies": {
    "@types/html-entities": "^1.2.16",
    "@types/lodash": "^4.14.121",
    "rimraf": "^2.6.3"
  },
  "scripts": {
    "clean": "rimraf ../client/server",
    "build": "npm run clean && npm run compile",
    "compile": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc -p .",
    "watch": "installServerIntoExtension ../client ./package.json ./tsconfig.json && tsc --watch -p ."
  }
}
